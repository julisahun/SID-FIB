        <rdfs:range rdf:resource="http://mapa#Agent"/>
        <rdfs:comment>Mark a resource as being carried by an agent</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mapa#IndendsToUnlock -->

    <owl:ObjectProperty rdf:about="http://mapa#IndendsToUnlock">
        <rdfs:domain rdf:resource="http://mapa#Agent"/>
        <rdfs:range rdf:resource="http://mapa#Cell"/>
        <rdfs:comment>Intended for orders between BDI and Situated agent. Indicates that the Situated agent should try to unlock a cell. Assign the cell that needs to be unlocked so you can validate in the Situated agent that you are at the right position.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mapa#IntendsToEmptyBackPack -->

    <owl:ObjectProperty rdf:about="http://mapa#IntendsToEmptyBackPack">
        <rdfs:domain rdf:resource="http://mapa#Agent"/>
        <rdfs:range rdf:resource="http://mapa#Cell"/>
        <rdfs:comment>Intended for orders between BDI and Situated agent. Indicates that the Situated agent should try to empty its backpack. Assign the cell where the agent needs to empty so you can validate in the Situated agent that you are at the right position.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mapa#IntendsToPickUp -->

    <owl:ObjectProperty rdf:about="http://mapa#IntendsToPickUp">
        <rdfs:domain rdf:resource="http://mapa#Agent"/>
        <rdfs:range rdf:resource="http://mapa#Cell"/>
        <rdfs:comment>Intended for orders between BDI and Situated agent. Indicates that the Situated agent should try to pick up its allowed resource from a cell. Assign the cell that needs to be emptied so you can validate in the Situated agent that you are at the right position.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mapa#IntendsToWalkTo -->

    <owl:ObjectProperty rdf:about="http://mapa#IntendsToWalkTo">
        <rdfs:domain rdf:resource="http://mapa#Agent"/>
        <rdfs:range rdf:resource="http://mapa#Cell"/>
        <rdfs:comment>Intended for orders between BDI and Situated agent. Indicates that the Situated agent should intend to walk to another node. We suggest you use it to indicate the next node in your path, instead of the overall destination.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://mapa#LocatedAt -->

    <owl:ObjectProperty rdf:about="http://mapa#LocatedAt">

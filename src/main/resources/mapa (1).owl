        <rdfs:comment xml:lang="en">Agent Storage</rdfs:comment>
        <rdfs:label xml:lang="en">Storage</rdfs:label>
    </owl:Class>
    


    <!-- http://mapa#Well -->

    <owl:Class rdf:about="http://mapa#Well">
        <rdfs:subClassOf rdf:resource="http://mapa#Cell"/>

        <rdfs:label xml:lang="en">Well</rdfs:label>
    </owl:Class>
    


    <!-- http://mapa#Windy -->

    <owl:Class rdf:about="http://mapa#Windy">
        <rdfs:subClassOf rdf:resource="http://mapa#Cell"/>

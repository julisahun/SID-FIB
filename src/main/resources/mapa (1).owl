        <rdfs:domain rdf:resource="http://mapa#Agent"/>
        <rdfs:range rdf:resource="http://mapa#Cell"/>
        <rdfs:comment>Location of the agent, or a mineral that has not been collected.</rdfs:comment>
    </owl:ObjectProperty>
    



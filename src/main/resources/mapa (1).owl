        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment>Amount of the resource being carried by an agent or cell.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://mapa#goldCapacity -->

    <owl:DatatypeProperty rdf:about="http://mapa#goldCapacity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://mapa#Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment>Total capacity of this resource of an agent. That is, the getBackPackFreeSpace amount at the start of the program. -1 represents no capacity and no hability to use/unlock this resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://mapa#lastUpdate -->

    <owl:DatatypeProperty rdf:about="http://mapa#lastUpdate">

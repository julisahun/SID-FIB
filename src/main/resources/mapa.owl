        <rdfs:label xml:lang="en">Open</rdfs:label>
    </owl:Class>
    


    <!-- http://mapa#Recollector -->

    <owl:Class rdf:about="http://mapa#Recollector">
        <rdfs:subClassOf rdf:resource="http://mapa#Agent"/>
